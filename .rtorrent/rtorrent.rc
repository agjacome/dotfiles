download_rate   = 0
upload_rate     = 57
max_peers       = 100
max_peers_seed  = 100
max_uploads     = 13
peer_exchange   = yes
port_random     = no
port_range      = 49166-49166
dht             = auto
dht_port        = 49167

check_hash      = yes
encoding_list   = UTF-8

directory       = /home/agjacome/.rtorrent
session         = /home/agjacome/.rtorrent
